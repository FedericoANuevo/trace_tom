pro interpol_fl, x=x, y=y, xi=xi, yi=yi, $
                 aia = aia, euvia = euvia, euvib = euvib, eit = eit, $
                 mk4 = mk4, kcor = kcor, lascoc2 = lascoc2

  if keyword_set(aia) then begin
     xi_min = 1.02
     xi_max = 1.25
     Nxi    = 50
  endif

  dxi = (xi_max-xi_min)/Nxi  
  xi  = xi_min + dxi/2. + dxi * findgen(Nxi)
  yi  = INTERPOL( y, x, xi )

  return
end
